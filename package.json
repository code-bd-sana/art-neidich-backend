{
  "name": "artneidich",
  "version": "1.0.0",
  "description": "Backend for Artneidich application",
  "license": "ISC",
  "author": "Artur Neidich",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "merge-all": "bash -lc \"cur=$(git branch --show-current); for b in $(git for-each-ref --format='%(refname:short)' refs/heads/ | grep -vE '^(main|development)$'); do git checkout \\\"$b\\\" && git merge \\\"$cur\\\"; done && git push --all\"",
    "merge-all-local": "bash -lc \"cur=$(git branch --show-current); for b in $(git for-each-ref --format='%(refname:short)' refs/heads/ | grep -vE '^(main|development)$'); do git checkout \\\"$b\\\" && git merge \\\"$cur\\\"; done\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "seed": "node src/seeders/seed.js",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.432.0",
    "@aws-sdk/client-ses": "^3.974.0",
    "@aws-sdk/lib-storage": "^3.432.0",
    "@aws-sdk/s3-request-presigner": "^3.972.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.9",
    "express": "^5.2.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nanoid": "^5.1.6",
    "nodemailer": "^7.0.12",
    "nodemon": "^3.1.11",
    "puppeteer": "^24.34.0",
    "streamifier": "^0.1.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0"
  }
}
